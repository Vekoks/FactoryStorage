<Window x:Class="FactoryStorage.View.LoadScheme"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FactoryStorage.View"
        mc:Ignorable="d"
        Title="Зареди схема" Height="253" Width="362"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="buttonLoad" Content="Зареди" HorizontalAlignment="Left" Margin="86,160,0,0" VerticalAlignment="Top" Height="34" Width="72" FontSize="14" Click="buttonLoad_Click"/>
        <ComboBox x:Name="comboBoxScheme" HorizontalAlignment="Center" Margin="0,103,0,0" VerticalAlignment="Top" Width="124" Height="28"/>
        <Label x:Name="labelHeading" Content="Зареждане на схема" HorizontalAlignment="Center" Margin="0,30,0,0" VerticalAlignment="Top" Height="40" Width="190" FontSize="18"/>
        <Button x:Name="buttonBack" Content="Назад" HorizontalAlignment="Left" Margin="200,160,0,0" VerticalAlignment="Top" Height="34" Width="66" FontSize="14" Click="buttonBack_Click"/>

    </Grid>
</Window>
